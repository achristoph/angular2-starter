{
  "extends": "tslint:latest",
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ],
  "rules": {
    "no-console": [
      false
    ],
    "ordered-imports": false,
    "member-access": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ]
  }
}