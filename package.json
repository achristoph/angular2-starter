{
  "name": "angular2starter",
  "version": "1.0.0",
  "description": "Angular2 Starter",
  "repository": "https://github.com/achristoph/angular2-starter",
  "license": "MIT",
  "scripts": {
    "global": "npm i -g typings karma",
    "typings": "typings install",
    "build": "node_modules/.bin/rimraf dist && node_modules/.bin/webpack  --config config/webpack.prod.js --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "start": "node_modules/.bin/webpack-dev-server --inline --colors --progress --display-error-details --display-cached",
    "server": "npm run build && node_modules/.bin/http-server ./dist",
    "test": "karma start --no-single-run --auto-watch",
    "doc": "node_modules/.bin/typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^2.2.4",
    "codelyzer": "1.0.0-beta.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "tslint-eslint-rules": "^1.5.0",
    "tslint-loader": "^2.1.5",
    "typedoc": "^0.4.5",
    "typescript": "^2.0.2",
    "typings": "^1.3.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@ngrx/core": "^1.1.0",
    "@ngrx/store": "^2.1.2",
    "core-js": "^2.4.1",
    "material-design-lite": "^1.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23"
  }
}
