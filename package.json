{
  "name": "angular2starter",
  "version": "1.0.0",
  "description": "Angular2 Starter",
  "scripts": {
    "global": "npm i -g typings karma",
    "typings": "typings install",
    "build": "node_modules/.bin/rimraf dist && node_modules/.bin/webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "start": "node_modules/.bin/webpack-dev-server --inline --colors --progress --display-error-details --display-cached",
    "server": "npm run build && node_modules/.bin/http-server ./dist",
    "test": "karma start --no-single-run --auto-watch",
    "doc": "node_modules/.bin/typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/"
  },
  "devDependencies": {
    "a2-in-memory-web-api": "^0.1.17",
    "codelyzer": "0.0.19",
    "copy": "^0.2.0",
    "copy-webpack-plugin": "^1.1.1",
    "core-js": "^2.4.0",
    "css-loader": "^0.23.1",
    "es6-promise": "3.0.2",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "http-server": "^0.9.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha-reporter": "^2.0.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.10.2",
    "tslint-eslint-rules": "^1.3.0",
    "tslint-loader": "^2.1.4",
    "typedoc": "^0.4.4",
    "typescript": "^1.8.10",
    "typings": "^0.7.9",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "es6-shim": "^0.35.0",
    "express": "^4.14.0",
    "material-design-lite": "^1.1.3",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "0.6.12"
  }
}
